namespace Neodroid.FBS;

table FBSUnobservables {
  poses:[FBSQuaternionTransform];
  bodies:[FBSBody];
}

struct FBSVector3 {
  x:double;
  y:double;
  z:double;
}

struct FBSQuaternion {
  x:double;
  y:double;
  z:double;
  w:double;
}

struct FBSQuaternionTransform{
  position:FBSVector3;
  rotation:FBSQuaternion;
}

struct FBSBody{
  velocity:FBSVector3;
  angular_velocity:FBSVector3;
}

file_identifier "SHAR";
